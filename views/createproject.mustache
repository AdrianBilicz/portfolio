<!DOCTYPE html>
<html dir="ltr" lang="en-US">
<head>
	{{>head}}
</head>

<body class="stretched">

<!-- Contact Form
	============================================= -->

	<div class="container">

	<h3>Create Project</h3>
		<br>
		<hr>
		<form class="form-row" action="/project" method="post">

			<div class="form-group col-md-12">
				<label for="name">Name <small>*</small></label>
				<input type="text" id="name" name="name" value="" class="form-control required" />
			</div>
			<div class="form-group col-md-12">
				<label for="url">url <small>*</small></label>
				<input type="text" id="url" name="url" value="" class="form-control required" />
			</div>
			<div class="form-group col-md-12">
				<label for="image">Image <small>*</small></label>
				<input type="text" name="image" value="" class="required form-control" />
			</div>
			<div class="form-group col-md-12">
				<label for="thumb">Thumb <small>*</small></label>
				<input type="text" name="thumb" value="" class="required form-control" />
			</div>

			<div class="form-group col-md-12">
				<label for="phone">Tools</label>
				<input type="text" id="phone" name="tools" value="" class="form-control" />
			</div>

			<div class="form-group col-md-12">
				<label for="message">Description <small>*</small></label>
				<textarea class="required form-control" id="message" name="description" rows="6" cols="30"></textarea>
			</div>

			<div class="col-md-12">
				<button class="btn btn-info" type="submit" id="submit" name="submit" value="submit">Send Message</button>
			</div>

		</form>
	</div>


</body>
</html>